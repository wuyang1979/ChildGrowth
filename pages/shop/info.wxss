page,
.page {
    background-color: #f5f5f5;
}

.shop-container {
    padding-bottom: 30px;
}

.logo {
    width: 70px;
    height: 70px;
    border-radius: 50%;
    margin-left: 40px;
    border: 2px solid white;
    margin-right: 15px;
}

.auth-area {
    display: flex;
    background-color: white;
    border-radius: 20px;
    padding: 2px 15px;
    align-items: center;
}

.auth-image {
    width: 15px;
    height: 15px;
    width: 13px;
    height: 13px;
    margin-right: 2px;
}

.auth-tip {
    font-size: 11px;
}

.shop-name {
    margin-bottom: 10px;
    font-size: 17px;
}

.product-item {
    display: inline-block;
    width: 50%;
}

.product-item-image {
    height: 120px;
    width: 100%;
}

.shop-product-area {
    display: flex;
    flex-wrap: wrap;
    padding: 20px;
}

.product-item {
    width: 48%;
    margin-bottom: 15px;
    border-radius: 10px;
    background-color: white;
    padding-bottom: 10px;
}

.product-item-image {
    height: 180px;
    width: 100%;
    border-radius: 10px 10px 0 0;
}

.product-item-name {
    font-size: 12px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    width: 85%;
    margin: 0 auto;
    margin-top: 5px;
    min-height: 38.4px;
}

.product-item-bottom {
    display: flex;
    align-items: center;
    width: 85%;
    margin: 0 auto;
    margin-top: 5px;
}

.product-item-proce {
    color: red;
    font-size: 15px;
}

.product-item-buy-count {
    font-size: 12px;
    margin-left: auto;
    margin-right: 5px;
    color: #ff6889;
    border: 1px solid #ff6889;
    letter-spacing: 1px;
    padding: 0 8px;
}

.product-item:nth-child(2n) {
    margin-left: auto;
}

.bottom-button {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 50px;
    background-color: white;
    border-top: 1px solid #e4e4e4;
    box-sizing: border-box;
    display: flex;
}

.bottom-left-button,
.bottom-middle-button,
.bottom-right-button {
    display: inline-flex;
    text-align: center;
    font-size: 17px;
    align-items: center;
}

.bottom-left-button {
    position: relative;
    width: 30%;
    justify-content: center;
    box-sizing: border-box;
    background-color: white;
}

.bottom-left-button::after {
    border: none;
}

.bottom-middle-button {
    width: 30%;
    justify-content: center;
}

.activity-bottom-left {
    width: 50%;
}

.share::after {
    content: '';
    position: absolute;
    left: auto;
    top: auto;
    bottom: 13px;
    right: 0;
    height: 50%;
    width: 1px;
    background-color: #ececec;
}

.bottom-right-button {
    background-color: #0198e7;
    color: white;
    font-size: 18px;
    margin-left: auto;
    width: 40%;
    justify-content: center;
}

.bottom-left-image {
    width: 25px;
    height: 25px;
    margin-right: 5px;
}

.bottom-middle-image {
    width: 22px;
    height: 22px;
    margin-right: 5px;
}

.bottom-logo-image {
    display: block;
    margin: 0 auto;
    width: 70px;
    height: 20px;
    margin-top: 30px;
}

.bottom-logo-tip {
    font-size: 12px;
    color: #b5b5b5;
    text-align: center;
    letter-spacing: 4px;
    padding-bottom: 20px;
}

.shop-top-area {
    display: flex;
    height: 130px;
    align-items: center;
    background: url("data:image/png;base64,") no-repeat center/100% 100%;
}