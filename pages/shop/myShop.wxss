page,
.page {
    background-color: #fcfaff;
}

.shop-container {
    padding-bottom: 50px;
}

.logo {
    width: 60px;
    height: 60px;
    border-radius: 50%;
    margin-left: 40px;
    border: 2px solid white;
    margin-right: 15px;
}

.auth-area {
    display: flex;
    background-color: white;
    border-radius: 20px;
    padding: 2px 15px;
    align-items: center;
}

.auth-image {
    width: 13px;
    height: 13px;
    margin-right: 5px;
    margin-left: 10px;
}

.see-image {
    width: 15px;
    height: 10px;
    margin-right: 5px;
}

.auth-tip {
    font-size: 11px;
    color: white;
}

.see-tip {
    font-size: 11px;
    color: #8c7305;
}

.shop-bottom-right-area {
    display: flex;
    align-items: baseline;
}

.shop-top-right-area {
    display: flex;
    align-items: center;
    margin-bottom: 7px;
}

.shop-name {
    font-size: 17px;
}

.shop-container-cover {
    background-color: #fcfaff;
    margin-top: -10px;
    border-radius: 20px 20px 0 0;
}

.newest-activity {
    font-size: 18px;
    padding: 10px 20px 0 20px;
    border-radius: 10px;
    letter-spacing: 2px;
    position: relative;
}

.newest-activity::before {
    content: '';
    position: absolute;
    left: auto;
    top: 15px;
    height: 20px;
    width: 4px;
    background-color: #ff8c22;
    border-radius: 5px;
}

.product-item {
    display: inline-block;
    width: 50%;
}

.product-item-image {
    height: 120px;
    width: 100%;
}

.shop-product-area {
    display: flex;
    flex-wrap: wrap;
    padding: 10px 20px 20px 20px;
}

.newest-tip {
    margin-left: 10px;
}

.product-item {
    width: 48%;
    margin-bottom: 15px;
    border-radius: 10px;
    background-color: white;
    padding-bottom: 10px;
}

.product-item-image {
    height: 180px;
    width: 100%;
    border-radius: 10px 10px 0 0;
}

.product-item-name {
    font-size: 12px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    width: 85%;
    margin: 0 auto;
    margin-top: 5px;
    min-height: 38.4px;
}

.product-item-bottom {
    display: flex;
    align-items: center;
    width: 85%;
    margin: 0 auto;
    margin-top: 5px;
}

.product-item-proce {
    color: red;
    font-size: 15px;
}

.product-item-buy-count {
    font-size: 12px;
    margin-left: auto;
    margin-right: 5px;
    color: #ff6889;
    border: 1px solid #ff6889;
    letter-spacing: 1px;
    padding: 0 8px;
}

.product-item:nth-child(2n) {
    margin-left: auto;
}

.bottom-button {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 50px;
    background-color: white;
    border-top: 1px solid #e4e4e4;
    box-sizing: border-box;
    display: flex;
}

.bottom-left-button,
.bottom-middle-button,
.bottom-right-button {
    text-align: center;
    font-size: 11px;
}

.bottom-left-button {
    position: relative;
    width: 50%;
    box-sizing: border-box;
    background-color: white;
}

.bottom-left-button::after {
    border: none;
}

.bottom-middle-button {
    width: 50%;
}

.selected {
    color: #ff8c21;
}

.share::after {
    content: '';
    position: absolute;
    left: auto;
    top: auto;
    bottom: 13px;
    right: 0;
    height: 50%;
    width: 1px;
    background-color: #ececec;
}

.bottom-right-button {
    background-color: #0198e7;
    color: white;
    font-size: 18px;
    margin-left: auto;
    width: 40%;
    justify-content: center;
}

.bottom-left-image {
    width: 25px;
    height: 25px;
    margin-bottom: -4px;
    margin-top: 3px;
}

.bottom-middle-image {
    width: 25px;
    height: 25px;
    margin-bottom: -4px;
    margin-top: 3px;
}

.bottom-logo-image {
    display: block;
    margin: 0 auto;
    width: 70px;
    height: 20px;
    margin-top: 30px;
}

.distribution-partner-image {
    display: block;
    width: 90%;
    margin: 0 auto;
    margin-top: 10px;
    margin-bottom: 10px;
}

.bottom-logo-tip {
    font-size: 12px;
    color: #b5b5b5;
    text-align: center;
    letter-spacing: 4px;
    padding-bottom: 20px;
}

.order-tip::before {
    content: '';
    position: absolute;
    left: 5px;
    top: 12px;
    height: 20px;
    width: 5px;
    background-color: #ff8c22;
    border-radius: 5px;
}

.right-arrow1 {
    width: 15px;
    height: 15px;
    margin-left: auto;
    margin-right: 20px;
}

.user-info {
    display: flex;
    align-items: center;
    padding-left: 5px;
}

.user-image {
    width: 80px;
    height: 80px;
    border-radius: 50%;
    border: 3px solid #fff;
    box-sizing: border-box;
    background-color: #ececec;
    margin-right: 20px;
}

.user-phone {
    font-size: 12px;
}

.opacity {
    opacity: 0.5;
}

.user-name {
    font-size: 18px;
}

.order-container,
.coopreate-container {
    padding: 6px 0px 0px 0px;
}

.order-area {
    background-color: white;
    border-radius: 5px;
    padding: 5px 15px 5px 15px;
}

.coopreate-area {
    background-color: white;
    border-radius: 5px;
    box-shadow: 0 3px 4px -2px #d1d1d1;
}

.order-tip {
    font-size: 16px;
    font-family: fantasy;
    padding: 10px 15px;
    position: relative;
}

.order-bottom-area {
    display: flex;
    align-items: center;
    padding: 10px 15px 10px 15px;
}

.growth-value,
.certificate-num {
    color: #ff8a1c;
    margin-top: -5px;
    margin-bottom: 5px;
}

.growth-center-bottom-area {
    display: flex;
    align-items: flex-end;
    padding: 10px 15px 10px 15px;
}

.coopreate-bottom-area {
    padding: 10px 15px 10px 20px;
}

.all-image,
.pending-pay-image,
.pending-receive-image,
.finished-image {
    width: 23px;
    height: 23px;
}

.all,
.pending-pay,
.pedding-receive,
.finished {
    width: 25%;
    text-align: center;
}

.coopreate-item {
    text-align: center;
    display: flex;
    align-items: center;
    border-bottom: 1px solid #eee;
    padding-bottom: 10px;
    padding-top: 10px;
}

.coopreate-item:last-child {
    border-bottom: none;
    padding-bottom: 0;
}

.coopreate-item:first-child {
    padding-top: 0;
}

.coopreate-navigator {
    width: 100%;
}

.all-tip,
.pending-pay-tip,
.pending-receive-tip,
.finished-tip {
    font-size: 13px;
    color: #676767;
}

.coopreate-item-tip {
    font-size: 13px;
    color: #676767;
}

.bottom-area {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 15px;
}

.bottom-tip {
    font-size: 12px;
    color: #b5b5b5;
}

.bottom-logo-image {
    display: block;
    margin: 0 auto;
    width: 70px;
    height: 20px;
    margin-top: 30px;
}

.bottom-logo-tip {
    font-size: 12px;
    color: #b5b5b5;
    text-align: center;
    letter-spacing: 4px;
    padding-bottom: 20px;
}

.order-line {
    background: #e0e0e0;
    width: 35%;
    height: 1px;
}

.coopreate-item-img {
    width: 20px;
    height: 20px;
    margin-right: 15px;
}

.right-arrow2 {
    width: 12px;
    height: 12px;
    margin-left: auto;
}

.growth-process-image,
.growth-portrait-image {
    width: 23px;
    height: 23px;
    margin-top: -5px;
}


.cover-area {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 10;
}

.distribution-partner-poster {
    display: block;
    margin: 0 auto;
    margin-top: 20rpx;
    width: 80%;
}

.close-distribution-partner-area {
    width: 80%;
    margin: 0 auto;
    margin-top: 140rpx;
    text-align: right;
}

.close-distribution-partner {
    width: 30px;
    height: 30px;
}

.top-area {
    display: flex;
    align-items: center;
    padding: 20px 10px 20px 20px;
    background: url("data:image/jpg;base64,") no-repeat center/100% 100%;
}

.shop-top-area {
    display: flex;
    height: 130px;
    align-items: center;
    background: url("data:image/jpg;base64,") no-repeat center/100% 100%;
}