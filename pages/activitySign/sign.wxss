page {
    height: 100%;
}

.phone-area {
    position: fixed;
    top: 430rpx;
    left: 100rpx;
    width: 75%;
    height: 230px;
}

.sign-image {
    width: 30px;
    height: 30px;
    position: absolute;
    right: 30rpx;
    top: 30rpx;
}

.tip {
    text-align: center;
    letter-spacing: 2px;
    font-size: 15px;
    padding-top: 25px;
}

.middle-area {
    display: flex;
    align-items: center;
    padding: 30px 15px 10px 15px;
}

.phone-image {
    width: 30px;
    height: 30px;
    margin-right: 10px;
}

.phone-input {
    font-size: 14px;
    letter-spacing: 1px;
    width: 70%;
}

.confirm {
    background-color: #ff8a19;
    width: 70%;
    margin: 0 auto;
    text-align: center;
    color: white;
    padding: 7px 10px;
    border-radius: 10px;
}

.line {
    background: #E0E3DA;
    width: 90%;
    height: 1px;
    margin: 0 auto;
    margin-bottom: 20px;
}

.activity-container {
    height: 100%;
    background-size: 100% 100%;
    background-image: url('data:image/jpg;base64,');
}