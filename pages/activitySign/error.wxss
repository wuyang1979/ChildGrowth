.image-area {
    display: flex;
    justify-content: center;
    margin-top: 50px;
}

.error-image {
    width: 120px;
    height: 120px;
}

.title {
    font-size: 21px;
    font-weight: bold;
    letter-spacing: 2px;
    font-family: monospace;
    text-align: center;
    margin-top: 20px;
}

.tip {
    text-align: center;
    margin-top: 10px;
    color: #7b7b7b;
    letter-spacing: 1px;
}

.go-back-btn {
    width: 70%;
    background-color: #ff8a19;
    color: white;
    text-align: center;
    margin: 0 auto;
    padding: 11px 15px;
    border-radius: 5px;
    letter-spacing: 3px;
    font-size: 17px;
    margin-top: 50px;
}

.to-index-btn {
    width: 70%;
    background-color: #fbfbfd;
    color: #a2a2a2;
    text-align: center;
    margin: 0 auto;
    padding: 11px 15px;
    border-radius: 5px;
    letter-spacing: 3px;
    font-size: 17px;
    margin-top: 10px;
    border: 1px solid #dadada;
}

